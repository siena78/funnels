//Rewrite to WWW
Options +FollowSymLinks
RewriteEngine on
RewriteCond %{HTTP_HOST} ^a.siena.pl[nc]
RewriteRule ^(.*)$ http://www.a.siena.pl/$1 [r=301,nc]

//Change default directory page
DirectoryIndex inviteonly.html

//Prevent viewing of .htaccess file
<Files .htaccess>
order allow,deny
deny from all
</Files>

//Prevent directory listings
Options All -Indexes
 